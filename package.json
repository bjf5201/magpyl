{
  "name": "magpyl",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+github.com:bjf5201/magpyl.git"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "automd && eslint . --fix && prettier --write .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm build && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run --coverage",
    "release:test": "pnpm lint && pnpm typecheck && pnpm test:unit"
  },
  "keywords": [],
  "author": "Bethany Fannin <contact@bethanyfannin.com> (https://www.bethanyfannin.com)",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@types/node": "22.15.18",
    "@vitest/coverage-v8": "3.1.3",
    "automd": "0.4.0",
    "changelogen": "0.6.1",
    "eslint": "9.26.0",
    "eslint-config-unjs": "0.4.2",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "renovate": "40.11.17",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitest": "3.1.3"
  }
}
